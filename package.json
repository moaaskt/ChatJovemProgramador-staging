{
  "name": "chatjovemprogramador-staging",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "npm run lint:js && npm run lint:py",
    "lint:js": "eslint \"static/js/**/*.js\"",
    "fix:js": "eslint \"static/js/**/*.js\" --fix",
    "format:js": "prettier --write \"static/js/**/*.js\"",
    "lint:py": "ruff check .",
    "fix:py": "ruff check . --fix",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test tests/e2e/chat.spec.ts",
    "prepare": "playwright install"
  },
  "devDependencies": {
    "@playwright/test": "^1.47.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.3"
  }
}